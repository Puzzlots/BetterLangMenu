{
  "formatVersion": 3,
  "display-name": "${mod_name}",
  "id": "${mod_id}",
  "version": "${mod_version}",
  "description": "${mod_desc}.",
  "authors": [ "CrabKing", "Spicylemon" ],
  "meta": {
    "icon": "${mod_id}:icons/icon.png"
  },
  "entrypoints": {
    "init": [
      "org.example.exmod.ExampleMod"
    ],
    "client-init": [
      "org.example.exmod.ClientInitializer"
    ]
  },
  "depends": {
    "cosmic-reach": ">=${cosmic_reach_minimum}"
  },
  "mixins": [
    "${mod_id}.mixins.json",
    {
      "config": "${mod_id}.client.mixins.json",
      "environment": "client"
    }
  ],
  "access-writers": [
    "${mod_id}.manipulator"
  ]
}